				"app-plus": {
					"background": "transparent",
					"animationType": "fade-in",
					"popGesture": "none"
				}
			}
		},
	
		{
			"path": "pages/notFound/index",
			"style":{
				"navigationBarTitleText": "404"
			}
		}

	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-template",
		"navigationBarBackgroundColor": "#F8F8F8",
